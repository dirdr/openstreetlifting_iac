# Database configuration
DB_USER=appuser
DB_PASSWORD=apppassword
DB_NAME=appdb

# Port mappings (Host machine -> Container)
# The port on your HOST machine that maps to PostgreSQL container's internal port 5432
DB_HOST_PORT=5432

# The port on your HOST machine that maps to backend container's internal port 8080
# Change this if you want to run the backend on a different port on your host (e.g., 3000, 9000)
BACKEND_HOST_PORT=8080

# Application name (used in Traefik labels)
APP_NAME=openstreetlifting
